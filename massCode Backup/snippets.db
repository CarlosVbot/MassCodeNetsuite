            fieldId:  'custrecord_efx_des_beca'\r\n            });\r\n\r\n            var cupon = currentRecord.getValue({\r\n                fieldId: 'custrecord_efx_des_tipo_cupon'\r\n            });\r\n\r\n            var inscrip = currentRecord.getValue({\r\n                fieldId: 'custrecord_efx_des_inscipcion'\r\n            });\r\n\r\n            var variable = currentRecord.getValue({\r\n                fieldId: 'custrecord_efx_des_variable'\r\n            });\r\n\r\n            var valorCupon = currentRecord.getValue({\r\n                fieldId: 'custrecord_efx_des_tipocupon_val'\r\n            });\r\n\r\n            var descripcion = currentRecord.getValue({\r\n                fieldId: 'custrecord_efx_des_descripcion'\r\n            });\r\n\r\n            var valorFijo = currentRecord.getValue({\r\n                fieldId: 'custrecord_efx_des_valor_2'\r\n            });\r\n\r\n            var activar = currentRecord.getValue({\r\n                fieldId: 'custrecord_efx_des_activo'\r\n            });\r\n\r\n            var ref = currentRecord.getValue({\r\n                fieldId: 'custrecord_efx_des_ref_num'\r\n            });\r\n\r\n\r\n            var refval = currentRecord.getValue({\r\n                fieldId: 'custrecord_efx_des_ref_var'\r\n            });\r\n\r\n            var promo = currentRecord.getValue({\r\n                fieldId: 'custrecord_efx_des_tipo_promo'\r\n            });\r\n            var sub=[1,2,3,4]\r\n            if(cole){\r\n                var nombre = busquedaNombre(tipo,cole);\r\n                var duplicado =  busquedaDuplicados(nombre,valorFijo);\r\n            }\r\n            if(beca){\r\n                var nombre = busquedaNombre(tipo,beca);\r\n                var duplicado =  busquedaDuplicados(nombre,valorVariable);\r\n            }\r\n            if(inscrip){\r\n                var nombre = busquedaNombre(tipo,inscrip);\r\n                var duplicado =  busquedaDuplicados(nombre,valorFijo);\r\n            }\r\n            if(variable){\r\n                var nombre = busquedaNombre(tipo,variable);\r\n                var duplicado =  busquedaDuplicados(nombre,valorVariable);\r\n            }\r\n            if(ref){\r\n                var nombre = busquedaNombre(tipo,ref);\r\n                var duplicado =  busquedaDuplicados(nombre,refval);\r\n            }\r\n            if(promo){\r\n                var nombre = busquedaNombre(tipo,promo);\r\n                var duplicado =  busquedaDuplicados(nombre,valorFijo);\r\n            }\r\n            if(cupon){\r\n                var nombre = busquedaNombre(tipo,cupon);\r\n                var duplicado =  busquedaDuplicados(nombre,valorCupon);\r\n            }\r\n\r\n            var cliente = currentRecord.getValue({\r\n                fieldId: 'custrecord_efx_des_cliente'\r\n            });\r\n            log.debug({\r\n                title: \"cliente \",\r\n                details: cliente\r\n            });\r\n            var recordsub = record.load({\r\n                type: \"customer\",\r\n                id: cliente\r\n            })\r\n            var idsub = recordsub.getValue({\r\n                fieldId:'subsidiary'\r\n            });\r\n\r\n            if(duplicado==true){\r\n                log.debug({\r\n                    title: \"duplicadof \",\r\n                    details: duplicado\r\n                });\r\n            }else{\r\n                log.debug({\r\n                    title: \"duplicadot \",\r\n                    details: duplicado\r\n                });\r\n                log.debug({\r\n                    title: \"cole \",\r\n                    details: cole\r\n                });\r\n                if(cole){\r\n\r\n                    var discountItem = record.create({type: record.Type.DISCOUNT_ITEM, isDynamic:false});\r\n                    discountItem.setText({fieldId:'itemid', text:nombre+' '+valorFijo+'.0%'});\r\n                    discountItem.setText({fieldId:'rate',text: valorFijo +'%'});\r\n                    discountItem.setValue({fieldId:'account',value:661});\r\n                    discountItem.setValue({fieldId:'description',value:descripcion});\r\n                    discountItem.setValue({fieldId:'subsidiary',value:sub});\r\n                    discountItem.setValue({fieldId:'includechildren',value:true});\r\n\r\n                    var iitem = discountItem.save();\r\n                    Iditem(record_noww,recType,iitem);\r\n\r\n                    /*var logsys = record.create({type: 'customrecordefx_inno_sys_log', isDynamic:true});\r\n                    logsys.setValue({fieldId:'custrecord_efx_log_descripcion',value: 'Creacion exitosa de descuento'});\r\n                    logsys.setValue({fieldId:'custrecord_efx_inno_log_timestamp',value: fecha});\r\n                    logsys.setValue({fieldId:'custrecord_efx_inno_log_ori',value: 4 });\r\n                    logsys.setValue({fieldId:'custrecord_efx_inno_log_mov',value: 6});\r\n                    //logsys.setValue({fieldId:'custrecord_efx_inno_log_tran',value: iitem});\r\n                    logsys.save();*/\r\n\r\n                }\r\n\r\n                if(beca){\r\n                    var discountItem = record.create({type: record.Type.DISCOUNT_ITEM, isDynamic:true});\r\n                    discountItem.setValue({fieldId:'itemid', value:nombre+' '+valorVariable+'.0%'});\r\n                    discountItem.setText({fieldId:'rate',text: valorVariable+'%'});\r\n                    discountItem.setValue({fieldId:'account',value:661});\r\n                    discountItem.setValue({fieldId:'description',value:descripcion});\r\n                    discountItem.setValue({fieldId:'subsidiary',value:sub});\r\n                    discountItem.setValue({fieldId:'includechildren',value:true});\r\n                    var iitem = discountItem.save();\r\n                    Iditem(record_noww,recType,iitem);\r\n                    var logsys = record.create({type: 'customrecordefx_inno_sys_log', isDynamic:true});\r\n                    logsys.setValue({fieldId:'custrecord_efx_log_descripcion',value: 'Creacion exitosa de descuento'});\r\n                    logsys.setValue({fieldId:'custrecord_efx_inno_log_timestamp',value: fecha});\r\n                    logsys.setValue({fieldId:'custrecord_efx_inno_log_ori',value: 4 });\r\n                    logsys.setValue({fieldId:'custrecord_efx_inno_log_mov',value: 6});\r\n                    // logsys.setValue({fieldId:'custrecord_efx_inno_log_tran',value: iitem});\r\n                    logsys.save();\r\n                }\r\n                if(inscrip){\r\n                    var discountItem = record.create({type: record.Type.DISCOUNT_ITEM, isDynamic:true});\r\n                    discountItem.setValue({fieldId:'itemid', value:nombre+' '+valorFijo+'.0%'});\r\n                    discountItem.setText({fieldId:'rate',text:valorFijo+'%'});\r\n                    discountItem.setValue({fieldId:'account',value:661});\r\n                    discountItem.setValue({fieldId:'description',value:descripcion});\r\n                    discountItem.setValue({fieldId:'subsidiary',value:sub});\r\n                    discountItem.setValue({fieldId:'includechildren',value:true});\r\n                    var iitem = discountItem.save();\r\n                    Iditem(record_noww,recType,iitem);\r\n                    var logsys = record.create({type: 'customrecordefx_inno_sys_log', isDynamic:true});\r\n                    logsys.setValue({fieldId:'custrecord_efx_log_descripcion',value: 'Creacion exitosa de descuento'});\r\n                    logsys.setValue({fieldId:'custrecord_efx_inno_log_timestamp',value: fecha});\r\n                    logsys.setValue({fieldId:'custrecord_efx_inno_log_ori',value: 4 });\r\n                    logsys.setValue({fieldId:'custrecord_efx_inno_log_mov',value: 6});\r\n                    //  logsys.setValue({fieldId:'custrecord_efx_inno_log_tran',value: iitem});\r\n                    logsys.save();\r\n                }\r\n                if(variable){\r\n                    var discountItem = record.create({type: record.Type.DISCOUNT_ITEM, isDynamic:true});\r\n                    discountItem.setValue({fieldId:'itemid', value:nombre+' '+valorVariable+'.0%'});\r\n                    discountItem.setText({fieldId:'rate',text: valorVariable+'%'});\r\n                    discountItem.setValue({fieldId:'account',value:661});\r\n                    discountItem.setValue({fieldId:'description',value:descripcion});\r\n                    discountItem.setValue({fieldId:'subsidiary',value:sub});\r\n                    discountItem.setValue({fieldId:'includechildren',value:true});\r\n                    var iitem = discountItem.save();\r\n                    Iditem(record_noww,recType,iitem);\r\n                    var logsys = record.create({type: 'customrecordefx_inno_sys_log', isDynamic:true});\r\n                    logsys.setValue({fieldId:'custrecord_efx_log_descripcion',value: 'Creacion exitosa de descuento'});\r\n                    logsys.setValue({fieldId:'custrecord_efx_inno_log_timestamp',value: fecha});\r\n                    logsys.setValue({fieldId:'custrecord_efx_inno_log_ori',value: 4 });\r\n                    logsys.setValue({fieldId:'custrecord_efx_inno_log_mov',value: 6});\r\n                    //logsys.setValue({fieldId:'custrecord_efx_inno_log_tran',value: iitem});\r\n                    logsys.save();     }\r\n                if(ref){\r\n                    var discountItem = record.create({type: record.Type.DISCOUNT_ITEM, isDynamic:true});\r\n                    discountItem.setValue({fieldId:'itemid', value:nombre+' '+refval+'.0%'});\r\n                    discountItem.setText({fieldId:'rate',text:refval+'%'});\r\n                    discountItem.setValue({fieldId:'account',value:661});\r\n                    discountItem.setValue({fieldId:'description',value:descripcion});\r\n                    discountItem.setValue({fieldId:'subsidiary',value:sub});\r\n                    discountItem.setValue({fieldId:'includechildren',value:true});\r\n\r\n                    var iitem = discountItem.save();\r\n                    Iditem(record_noww,recType,iitem);\r\n                    var logsys = record.create({type: 'customrecordefx_inno_sys_log', isDynamic:true});\r\n                    logsys.setValue({fieldId:'custrecord_efx_log_descripcion',value: 'Creacion exitosa de descuento'});\r\n                    logsys.setValue({fieldId:'custrecord_efx_inno_log_timestamp',value: fecha});\r\n                    logsys.setValue({fieldId:'custrecord_efx_inno_log_ori',value: 4 });\r\n                    logsys.setValue({fieldId:'custrecord_efx_inno_log_mov',value: 6});\r\n                    //logsys.setValue({fieldId:'custrecord_efx_inno_log_tran',value: iitem});\r\n                    logsys.save();\r\n                }\r\n                if(promo){\r\n                    var discountItem = record.create({type: record.Type.DISCOUNT_ITEM, isDynamic:true});\r\n                    discountItem.setValue({fieldId:'itemid', value:nombre+' '+valorFijo+'.0%'});\r\n                    discountItem.setText({fieldId:'rate',text:valorFijo+'%'});\r\n                    discountItem.setValue({fieldId:'account',value:661});\r\n                    discountItem.setValue({fieldId:'description',value:descripcion});\r\n                    discountItem.setValue({fieldId:'subsidiary',value:sub});\r\n                    discountItem.setValue({fieldId:'includechildren',value:true});\r\n                    var iitem = discountItem.save();\r\n                    Iditem(record_noww,recType,iitem);\r\n                    var logsys = record.create({type: 'customrecordefx_inno_sys_log', isDynamic:true});\r\n                    logsys.setValue({fieldId:'custrecord_efx_log_descripcion',value: 'Creacion exitosa de descuento'});\r\n                    logsys.setValue({fieldId:'custrecord_efx_inno_log_timestamp',value: fecha});\r\n                    logsys.setValue({fieldId:'custrecord_efx_inno_log_ori',value: 4 });\r\n                    logsys.setValue({fieldId:'custrecord_efx_inno_log_mov',value: 6});\r\n                    // logsys.setValue({fieldId:'custrecord_efx_inno_log_tran',value: iitem});\r\n                    logsys.save();\r\n                }\r\n                if(cupon){\r\n                    var discountItem = record.create({type: record.Type.DISCOUNT_ITEM, isDynamic:true});\r\n                    discountItem.setValue({fieldId:'itemid', value:nombre+' '+valorCupon+'.0%'});\r\n                    discountItem.setText({fieldId:'rate',text:valorCupon+'%'});\r\n                    discountItem.setValue({fieldId:'account',value:661});\r\n                    discountItem.setValue({fieldId:'description',value:descripcion});\r\n                    discountItem.setValue({fieldId:'subsidiary',value:sub});\r\n                    discountItem.setValue({fieldId:'includechildren',value:true});\r\n                    var iitem = discountItem.save();\r\n                    Iditem(record_noww,recType,iitem);\r\n                    var logsys = record.create({type: 'customrecordefx_inno_sys_log', isDynamic:true});\r\n                    logsys.setValue({fieldId:'custrecord_efx_log_descripcion',value: 'Creacion exitosa de descuento'});\r\n                    logsys.setValue({fieldId:'custrecord_efx_inno_log_timestamp',value: fecha});\r\n                    logsys.setValue({fieldId:'custrecord_efx_inno_log_ori',value: 4 });\r\n                    logsys.setValue({fieldId:'custrecord_efx_inno_log_mov',value: 6});\r\n                    // logsys.setValue({fieldId:'custrecord_efx_inno_log_tran',value: iitem});\r\n                    logsys.save();\r\n                }\r\n\r\n\r\n\r\n\r\n            }\r\n\r\n\r\n        }\r\n\r\n        function busquedaDuplicados(nombre,valor){\r\n            var name = nombre+' '+valor+'.0%';\r\n            var valtem;\r\n            log.debug({\r\n                title: \"name \",\r\n                details: name\r\n            });\r\n            log.debug({\r\n                title: \"valor \",\r\n                details: valor\r\n            });\r\n            var itemSearchObj = search.create({\r\n                type: \"item\",\r\n                filters:\r\n                    [\r\n                        [\"name\",\"is\",name]\r\n\r\n                    ],\r\n                columns:\r\n                    [\r\n                        search.createColumn({\r\n                            name: \"itemid\",\r\n                            sort: search.Sort.ASC,\r\n                            label: \"Name\"\r\n                        }),\r\n                        search.createColumn({name: \"displayname\", label: \"Display Name\"}),\r\n                        search.createColumn({name: \"baseprice\", label: \"Base Price\"}),\r\n\r\n                    ]\r\n            });\r\n            var searchResultCount = itemSearchObj.runPaged().count;\r\n            log.debug({\r\n                title: \"searchResultCount \",\r\n                details: searchResultCount\r\n            });\r\n            itemSearchObj.run().each(function(result){\r\n                valtem   = result.getValue({name: \"baseprice\"  });\r\n                return true;\r\n            });\r\n            log.debug({\r\n                title: \"valtem \",\r\n                details: valtem\r\n            });\r\n            if(searchResultCount > 0){\r\n                if((-1*parseFloat(valtem)) == parseFloat(valor)){\r\n                    return true;\r\n                }\r\n                return false;\r\n            }else{\r\n                return false;\r\n            }\r\n\r\n        }\r\n\r\n        function busquedaNombre(tipo,subtipo){\r\n\r\n            var results = [];\r\n            var valor  ;\r\n            if(tipo==1){\r\n\r\n                var customrecord_efx_des_inscripcionSearchObj = search.create({\r\n                    type: \"customrecord_efx_des_inscripcion\",\r\n                    filters:\r\n                        [\r\n                            [\"internalidnumber\",\"equalto\",subtipo]\r\n                        ],\r\n                    columns:\r\n                        [\r\n                            search.createColumn({\r\n                                name: \"name\",\r\n                                sort: search.Sort.ASC,\r\n                                label: \"Name\"\r\n                            }),\r\n                        ]\r\n                });\r\n\r\n                customrecord_efx_des_inscripcionSearchObj.run().each(function(result){\r\n                    valor = result.getValue({name: \"name\"  });\r\n                    return true;\r\n                });\r\n            }\r\n            if(tipo==2){\r\n\r\n                var customrecord_efx_des_tipo_colegiaturaSearchObj = search.create({\r\n                    type: \"customrecord_efx_des_tipo_colegiatura\",\r\n                    filters:\r\n                        [\r\n                            [\"internalidnumber\",\"equalto\",subtipo]\r\n                        ],\r\n                    columns:\r\n                        [\r\n                            search.createColumn({\r\n                                name: \"name\",\r\n                                sort: search.Sort.ASC,\r\n                                label: \"Name\"\r\n                            }),\r\n                        ]\r\n                });\r\n\r\n                customrecord_efx_des_tipo_colegiaturaSearchObj.run().each(function(result){\r\n                    valor = result.getValue({name: \"name\"  });\r\n                    return true;\r\n                });\r\n\r\n            }\r\n            if(tipo==3){\r\n\r\n                var customrecord_efx_des_becaSearchObj = search.create({\r\n                    type: \"customrecord_efx_des_beca\",\r\n                    filters:\r\n                        [\r\n                            [\"internalidnumber\",\"equalto\",subtipo]\r\n                        ],\r\n                    columns:\r\n                        [\r\n                            search.createColumn({\r\n                                name: \"name\",\r\n                                sort: search.Sort.ASC,\r\n                                label: \"Name\"\r\n                            }),\r\n\r\n                        ]\r\n                });\r\n\r\n                customrecord_efx_des_becaSearchObj.run().each(function(result){\r\n                    valor = result.getValue({name: \"name\"  });\r\n                    return true;\r\n                });\r\n\r\n            }\r\n            if(tipo==4){\r\n\r\n                var customrecord_efx_des_variableSearchObj = search.create({\r\n                    type: \"customrecord_efx_des_variable\",\r\n                    filters:\r\n                        [\r\n                            [\"internalidnumber\",\"equalto\",subtipo]\r\n                        ],\r\n                    columns:\r\n                        [\r\n                            search.createColumn({\r\n                                name: \"name\",\r\n                                sort: search.Sort.ASC,\r\n                                label: \"Name\"\r\n                            }),\r\n                        ]\r\n                });\r\n                customrecord_efx_des_variableSearchObj.run().each(function(result){\r\n                    valor = result.getValue({name: \"name\"  });\r\n                    return true;\r\n                });\r\n\r\n            }\r\n            if(tipo==5){\r\n\r\n                var customrecord_efx_des_tipo_cuponSearchObj = search.create({\r\n                    type: \"customrecord_efx_des_tipo_cupon\",\r\n                    filters:\r\n                        [\r\n                            [\"internalidnumber\",\"equalto\",subtipo]\r\n                        ],\r\n                    columns:\r\n                        [\r\n                            search.createColumn({\r\n                                name: \"name\",\r\n                                sort: search.Sort.ASC,\r\n                                label: \"Name\"\r\n                            }),\r\n                        ]\r\n                });\r\n\r\n                customrecord_efx_des_tipo_cuponSearchObj.run().each(function(result){\r\n                    valor  = result.getValue({name: \"name\"  });\r\n                    return true;\r\n                });\r\n            }\r\n            if(tipo==6){\r\n\r\n                var customrecord_efx_referidosSearchObj = search.create({\r\n                    type: \"customrecord_efx_referidos\",\r\n                    filters:\r\n                        [\r\n                            [\"internalidnumber\",\"equalto\",subtipo]\r\n                        ],\r\n                    columns:\r\n                        [\r\n                            search.createColumn({\r\n                                name: \"name\",\r\n                                sort: search.Sort.ASC,\r\n                                label: \"Name\"\r\n                            }),\r\n                        ]\r\n                });\r\n                customrecord_efx_referidosSearchObj.run().each(function(result){\r\n                    valor = result.getValue({name: \"name\"  });\r\n                    return true;\r\n                });\r\n            }\r\n            if(tipo==7){\r\n\r\n                var customrecord_efx_des_tipo_promoSearchObj = search.create({\r\n                    type: \"customrecord_efx_des_tipo_promo\",\r\n                    filters:\r\n                        [\r\n                            [\"internalidnumber\",\"equalto\",subtipo]\r\n                        ],\r\n                    columns:\r\n                        [\r\n                            search.createColumn({\r\n                                name: \"name\",\r\n                                sort: search.Sort.ASC,\r\n                                label: \"Name\"\r\n                            }),\r\n                        ]\r\n                });\r\n                customrecord_efx_des_tipo_promoSearchObj.run().each(function(result){\r\n                    valor = result.getValue({name: \"name\"  });\r\n                    return true;\r\n                });\r\n            }\r\n            if(tipo==''){\r\n                return false;\r\n            }\r\n\r\n\r\n            return valor;\r\n\r\n        }\r\n\r\n        function Iditem(record_noww,recType,iitem) {\r\n\r\n            var recordCreate = record.load({type: 'customrecord_efx_des_descuentos',id: parseInt(record_noww.id) });\r\n            recordCreate.setValue({fieldId:'custrecord_efx_id_item',value: iitem});\r\n            recordCreate.save();\r\n            log.audit({title:'parseInt(record_noww.id)',details:parseInt(record_noww.id)})\r\n            log.audit({title:'recordCreate',details:recordCreate})\r\n\r\n        }\r\n\r\n\r\n        return {\r\n            beforeLoad: beforeLoad,\r\n            afterSubmit: afterSubmit\r\n        }\r\n    });"}],"tags":[],"isFavorites":false,"isDeleted":false,"createdAt":{"$$date":1622161992281},"updatedAt":{"$$date":1622162024233},"_id":"2MrePUZZ5decgylq","folder":{"id":"eUKUysjGr","name":"UserEvent","open":false,"defaultLanguage":"text"},"tagsPopulated":[]}
