{"name":"runPaged","folderId":null,"content":[{"label":"Fragment 1","language":"javascript","value":"var ref_rec_search = search.create({\r\n                type: \"customrecord_efx_db_txt_referencia\",\r\n                filters:\r\n                    [\r\n                        [\"custrecord_efx_db_ref_item\",\"anyof\",iditem],\r\n                        \"AND\",\r\n                        [\"custrecord_efx_db_ref_customer\",\"anyof\",idalumno]\r\n                    ],\r\n                columns:\r\n                    [\r\n                        search.createColumn({name: \"custrecord_efx_db_ref_ref\", label: \"Referencia\"})\r\n                    ]\r\n            });\r\n            var searchResultCount = ref_rec_search.runPaged().count;\r\n            log.debug(\"Number of results\",searchResultCount);\r\n            ref_rec_search.run().each(function(result){\r\n                // .run().each has a limit of 4,000 results\r\n                var ref_alumno = result.getValue('custrecord_efx_db_ref_ref');\r\n                log.audit({title: 'La referencia para este alumno es', details:ref_alumno});\r\n                //Write Referencia bancaria field (custbody_ref_banc)\r\n                rec.setValue('custbody_ref_banc', ref_alumno);\r\n                //return true to continue with loop\r\n                return true;\r\n            });"}],"tags":[],"isFavorites":false,"isDeleted":false,"createdAt":{"$$date":1613404729229},"updatedAt":{"$$date":1613404740094},"_id":"7FHEtnFY1IIiyF81","tagsPopulated":[]}
