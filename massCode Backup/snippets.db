upon.updateDisplayType({\r\n                    displayType : serverWidget.FieldDisplayType.NODISPLAY\r\n                });\r\n                var inscrip = form.getField({\r\n                    id : 'custrecord_efx_des_inscipcion'\r\n                });\r\n                inscrip.updateDisplayType({\r\n                    displayType : serverWidget.FieldDisplayType.NODISPLAY\r\n                });\r\n                var variable = form.getField({\r\n                    id : 'custrecord_efx_des_variable'\r\n                });\r\n                variable.updateDisplayType({\r\n                    displayType : serverWidget.FieldDisplayType.NODISPLAY\r\n                });\r\n                var valorCupon = form.getField({\r\n                    id : 'custrecord_efx_des_tipocupon_val'\r\n                });\r\n                valorCupon.updateDisplayType({\r\n                    displayType : serverWidget.FieldDisplayType.NODISPLAY\r\n                });\r\n                var descripcion = form.getField({\r\n                    id : 'custrecord_efx_des_descripcion'\r\n                });\r\n                descripcion.updateDisplayType({\r\n                    displayType : serverWidget.FieldDisplayType.NODISPLAY\r\n                });\r\n                var valorFijo = form.getField({\r\n                    id : 'custrecord_efx_des_valor_2'\r\n                });\r\n                valorFijo.updateDisplayType({\r\n                    displayType : serverWidget.FieldDisplayType.NODISPLAY\r\n                });\r\n                var activar = form.getField({\r\n                    id : 'custrecord_efx_des_activo'\r\n                });\r\n                activar.updateDisplayType({\r\n                    displayType : serverWidget.FieldDisplayType.NODISPLAY\r\n                });\r\n                var ref = form.getField({\r\n                    id : 'custrecord_efx_des_ref_num'\r\n                });\r\n                ref.updateDisplayType({\r\n               