 = false;\r\n                referido.isDisplay = false;\r\n                records.setValue({fieldId: 'custrecord_efx_des_variable', value: DescuentoVariable2});\r\n            }\r\n            if (tipo == 5){\r\n                descripcion.isDisplay = true;\r\n                valorfijo.isDisplay = true;\r\n                promo.isDisplay = false;\r\n                DescuentoCupon.isDisplay = true;\r\n                CantidadCupon.isDisplay = true;\r\n                ValorVariable.isDisplay = false;\r\n                DescuentoColegiatura.isDisplay = false;\r\n                DescuentoBECA.isDisplay = false;\r\n                DescuentoInscripcion.isDisplay = false;\r\n                DescuentoVariable.isDisplay = false;\r\n\r\n                Activar.isDisplay = true;\r\n                Nreferido.isDisplay = false;\r\n                referido.isDisplay = false;\r\n                records.setValue({fieldId: 'custrecord_efx_des_tipo_cupon', value: DescuentoCupon2});\r\n            }\r\n            if (tipo == 6){\r\n                descripcion.isDisplay = true;\r\n                valorfijo.isDisplay = true;\r\n                promo.isDisplay = false;\r\n                DescuentoCupon.isDisplay = false;\r\n                CantidadCupon.isDisplay = false;\r\n                ValorVariable.isDisplay = false;\r\n                DescuentoColegiatura.isDisplay = false;\r\n                DescuentoBECA.isDisplay = false;\r\n                DescuentoInscripcion.isDisplay = false;\r\n                DescuentoVariable.isDisplay = false;\r\n\r\n                Activar.isDisplay = true;\r\n                Nreferido.isDisplay = true;\r\n                referido.isDisplay = true;\r\n                records.setValue({fieldId: 'custrecord_efx_des_ref_var', value: referido2});\r\n            }\r\n            if (tipo == 7){\r\n                descripcion.isDisplay = true;\r\n                valorfijo.isDisplay = true;\r\n                promo.isDisplay = true;\r\n                DescuentoCupon.isDisplay = false;\r\n                CantidadCupon.isDisplay = false;\r\n                ValorVariable.isDisplay = false;\r\n                DescuentoColegiatura.isDisplay = false;\r\n                DescuentoBECA.isDisplay = false;\r\n                DescuentoInscripcion.isDisplay = false;\r\n                DescuentoVariable.isDisplay = false;\r\n\r\n                Activar.isDisplay = true;\r\n                Nreferido.isDisplay = true;\r\n                referido.isDisplay = true;\r\n                records.setValue({fieldId: 'custrecord_efx_des_ref_var', value: promo2});\r\n            }\r\n            if (tipo == ''){\r\n\r\n            }\r\n        }\r\n\r\n        function saveRecord(scriptContext) {\r\n            var ValidacionCliente = currentRecord.get().getValue('custrecord_efx_des_cliente');\r\n            if(ValidacionCliente){\r\n                var tipo2 = currentRecord.get().getValue('custrecord_efx_des_tipo_2');\r\n                var Activar2 = currentRecord.get().getValue('custrecord_efx_des_activo');\r\n                var DescuentoInscripcion2 = currentRecord.get().getValue('custrecord_efx_des_inscipcion');\r\n                var ValorVariable2 = currentRecord.get().getValue('custrecord_efx_des_valormanual');\r\n                var DescuentoColegiatura2 = currentRecord.get().getValue('custrecord_efx_des_cole');\r\n                var DescuentoBECA2 = currentRecord.get().getValue('custrecord_efx_des_beca');\r\n                var DescuentoCupon2 = currentRecord.get().getValue('custrecord_efx_des_tipo_cupon');\r\n                var DescuentoVariable2 = currentRecord.get().getValue('custrecord_efx_des_variable');\r\n                var CantidadCupon2 = currentRecord.get().getValue('custrecord_efx_des_tipocupon_val');\r\n                var Fechafin2 = currentRecord.get().getValue('custrecord_efx_des_fecha_ter');\r\n                var Fechaini2 = currentRecord.get().getValue('custrecord_efx_des_fecha_ini');\r\n                var client = currentRecord.get().getValue('custrecord_efx_des_cliente');\r\n                var recordid = currentRecord.get().getValue('recordid');\r\n                var Nreferido = currentRecord.get().getValue('custrecord_efx_des_ref_num');\r\n                var referido = currentRecord.get().getValue('custrecord_efx_des_ref_var');\r\n                var promo2 = currentRecord.get().getValue('custrecord_efx_des_tipo_promo');\r\n\r\n                if(recordid){\r\n                    if (tipo2 == 1){\r\n\r\n                        if(DescuentoInscripcion2 =='' ){\r\n                            alert('Campos vacíos ')\r\n                            return false;\r\n                        }\r\n                    }\r\n                    if (tipo2 == 2){\r\n\r\n                        if(DescuentoColegiatura2 =='' ){\r\n                            alert('Campos vacíos ')\r\n                            return false;\r\n                        }\r\n                    }\r\n                    if (tipo2 == 3){\r\n\r\n                        if(DescuentoBECA2 =='' || DescuentoVariable2 ==''){\r\n                            alert('Campos vacíos ')\r\n                            return false;\r\n                        }\r\n                    }\r\n                    if (tipo2 == 4){\r\n\r\n                        if(ValorVariable2 =='' || DescuentoVariable2 =='' ){\r\n                            alert('Campos vacíos ')\r\n                            return false;\r\n                        }\r\n                    }\r\n                    if (tipo2 == 5){\r\n\r\n                        if(DescuentoCupon2 =='' || CantidadCupon2 == '' ){\r\n                            alert('Campos vacíos ')\r\n                            return false;\r\n                        }\r\n                    }\r\n                    if (tipo2 == 6){\r\n\r\n                        if(Nreferido =='' || referido == ''   ){\r\n                            alert('Campos vacíos ')\r\n                    